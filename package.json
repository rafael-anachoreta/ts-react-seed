{
  "name": "ts-react-seed",
  "version": "1.0.0",
  "main": "index.tsx",
  "repository": "https://github.com/rafael-anachoreta/ts-react-seed",
  "license": "MIT",
  "private": false,
  "scripts": {
    "commit": "git-cz",
    "build": "webpack --mode production",
    "prettier": "prettier -c",
    "lint": "eslint --ext .js,.jsx,.tsx --ignore-pattern *.css src/** --fix",
    "start": "webpack-dev-server --open --mode development",
    "test": "jest -c jest.config.js",
    "watch": "yarn test --watch",
    "cypress:ci": "server-test 8080 cypress",
    "cypress": "cypress run",
    "cypress:open": "cypress open",
    "storybook": "start-storybook -p 6006",
    "build-storybook": "build-storybook"
  },
  "devDependencies": {
    "@babel/core": "~7.8.4",
    "@babel/preset-env": "~7.8.4",
    "@babel/preset-react": "~7.8.3",
    "@storybook/addon-actions": "^6.3.4",
    "@storybook/addon-essentials": "^6.3.4",
    "@storybook/react": "^6.3.4",
    "@testing-library/jest-dom": "^5.1.1",
    "@testing-library/react": "^9.4.1",
    "@types/jest": "^25.1.3",
    "@types/node": "^13.7.6",
    "@types/react": "~16.9.23",
    "@types/react-dom": "~16.9.5",
    "@typescript-eslint/eslint-plugin": "^2.21.0",
    "@typescript-eslint/parser": "^2.21.0",
    "@typescript-eslint/typescript-estree": "^2.21.0",
    "babel-eslint": "~10.1.0",
    "babel-loader": "~8.0.6",
    "css-loader": "^3.4.2",
    "cypress": "^7.7.0",
    "cz-conventional-changelog": "3.1.0",
    "eslint": "^6.8.0",
    "eslint-config-prettier": "^6.10.0",
    "eslint-plugin-prettier": "~3.1.2",
    "eslint-plugin-react": "~7.18.3",
    "html-loader": "^0.5.5",
    "html-webpack-plugin": "^3.2.0",
    "husky": "^4.2.3",
    "jest": "^25.1.0",
    "prettier": "~1.19.1",
    "prettier-eslint": "^9.0.1",
    "react-docgen-typescript-loader": "~3.6.0",
    "start-server-and-test": "~1.10.8",
    "style-loader": "^1.1.3",
    "ts-loader": "^6.2.1",
    "ts-node": "~8.6.2",
    "typescript": "~3.8.2",
    "webpack": "~4.41.6",
    "webpack-cli": "~3.3.11",
    "webpack-dev-server": "~3.10.3"
  },
  "dependencies": {
    "react": "~16.13.0",
    "react-dom": "~16.13.0"
  },
  "resolutions": {
    "micromatch": "^4.0.2"
  },
  "husky": {
    "hooks": {
      "pre-push": "yarn test && yarn lint"
    }
  },
  "config": {
    "commitizen": {
      "path": "./node_modules/cz-conventional-changelog"
    }
  }
}
