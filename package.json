{
  "name": "ts-react-seed",
  "version": "1.0.0",
  "private": false,
  "main": "index.tsx",
  "repository": "https://github.com/rafael-anachoreta/ts-react-seed",
  "license": "MIT",
  "scripts": {
    "build": "webpack --mode production",
    "cypress": "cypress run",
