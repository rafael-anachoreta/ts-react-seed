    "start": "webpack serve --open --mode development",
    "prepare": "husky install"
  },
  "devDependencies": {
    "@babel/core": "~7.22.5",
    "@babel/preset-env": "~7.22.5",
    "@babel/preset-react": "~7.22.5",
    "@types/node": "^20.2.5",
    "@types/react": "^18.2.9",
    "@types/react-dom": "^18.2.5",
    "@typescript-eslint/eslint-plugin": "^5.59.9",
    "@typescript-eslint/parser": "^5.59.9",
    "@webpack-cli/serve": "^2.0.5",
    "autoprefixer": "^10.4.14",
    "babel-eslint": "~10.1.0",
    "babel-loader": "~9.1.2",
    "css-loader": "^6.8.1",
    "cypress": "^12.14.0",
    "eslint": "^8.42.0",
    "eslint-config-prettier": "^8.8.0",
    "eslint-plugin-react": "^7.32.2",
    "html-loader": "^4.2.0",
    "html-webpack-plugin": "^5.5.1",
    "husky": "^8.0.3",
    "lint-staged": "^13.2.2",
    "postcss-loader": "^7.3.3",
    "prettier": "^2.8.8",
    "react-docgen-typescript-loader": "~3.7.2",
    "start-server-and-test": "~2.0.0",
    "style-loader": "^3.3.3",
    "tailwindcss": "^3.3.2",
    "ts-loader": "^9.4.3",
    "ts-node": "~10.9.1",
    "typescript": "~5.1.3",
    "webpack": "^5.86.0",
    "webpack-cli": "^5.1.4",
    "webpack-dev-server": "^4.15.0"
  },
  "dependencies": {
    "react": "^18.2.0",
    "react-dom": "^18.2.0"
  },
  "lint-staged": {
    "src/**/*.{js,jsx,ts,tsx}": [
      "eslint --fix",
      "prettier --write"
    ],
    "*.{json,css,md}": [
      "prettier --write"
    ]
  }
